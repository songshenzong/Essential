{
  "name": "songshenzong/essentials",
  "description": "Essentials Methods for Laravel",
  "keywords": [
    "laravel",
    "response",
    "web"
  ],
  "license": "MIT",
  "homepage": "http://songshenzong.com",
  "support": {
    "issues": "https://github.com/songshenzong/essentials/issues",
    "source": "https://github.com/songshenzong/essentials"
  },
  "authors": [
    {
      "name": "Connor Niu",
      "email": "connor.niu@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "songshenzong/api": "^0.1.0@dev",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
    "illuminate/routing": "^5.1",
    "league/fractal": ">=0.12.0",
    "symfony/finder": "~2.7|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "^2.8",
    "illuminate/auth": "^5.1",
    "illuminate/cache": "^5.1",
    "illuminate/console": "^5.1",
    "illuminate/database": "^5.1",
    "illuminate/events": "^5.1",
    "illuminate/filesystem": "^5.1",
    "illuminate/log": "^5.1",
    "laravel/lumen-framework": "5.1.* || 5.2.*",
    "lucadegasperi/oauth2-server-laravel": "5.0.*",
    "mockery/mockery": "~0.9",
    "tymon/jwt-auth": "1.0.*"
  },
  "autoload": {
    "psr-4": {
